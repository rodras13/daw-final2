extends layout 

block content
    if styleTarget=='sw'
        if sw_test_items.length 
            header.sw
                .info
                    h1 Test de Star Wars
            main
                .form-container
                    form(action=``, method="get")
                        for sw in sw_test_items
                            fieldset
                                .item-container 
                                    .item-question
                                        legend #{sw.question}
                                    .item-answers 
                                        .item-disagree No estoy de acuerdo
